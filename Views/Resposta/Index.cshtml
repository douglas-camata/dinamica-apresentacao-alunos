@model IEnumerable<App.Models.Resposta>

@{
    ViewData["Title"] = "Index";
}

<h1>Alunos que jรก responderam</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td >
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RespostaId" class="btn btn-primary"><i class="bi bi-pencil-fill"></i> Editar </a>
            </td>
        </tr>
}
    </tbody>
</table>
